[package]
edition = "2018"
name = "{crate}"
version = "{version}"
authors = ["Henrik BÃ¶ving <hargonix@gmail.com>", "ht32-rs Contributors"]
description = "Device support crates for {family} devices"
repository = "https://github.com/ht32-rs/ht32-rs"
readme = "README.md"
keywords = ["ht32", "svd2rust", "no_std", "embedded"]
categories = ["embedded", "no-std"]
license = "MIT/Apache-2.0"

[dependencies]
bare-metal = "0.2.4"
vcell = "0.1.0"
cortex-m = ">=0.5.8,<0.7"

[dependencies.cortex-m-rt]
optional = true
version = "0.6.10"

[package.metadata.docs.rs]
features = {docs_features}
targets = []

[features]
default = []
rt = ["cortex-m-rt/device"]
{features}